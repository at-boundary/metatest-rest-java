# Metatest Configuration Example
# Copy this file to metatest.properties and configure your values

# API Configuration - Required
metatest.api.key=mt_proj_your_api_key_here
metatest.project.id=your-project-uuid-here
metatest.api.url=http://localhost:8080
metatest.config.source=local

# Alternative: Use environment variables or system properties
# Environment Variables:
#   METATEST_API_KEY=mt_proj_your_api_key_here
#   METATEST_PROJECT_ID=your-project-uuid-here
#   METATEST_API_URL=http://localhost:8080
#
# System Properties:
#   -Dmetatest.api.key=mt_proj_your_api_key_here
#   -Dmetatest.project.id=your-project-uuid-here
#   -Dmetatest.api.url=http://localhost:8080